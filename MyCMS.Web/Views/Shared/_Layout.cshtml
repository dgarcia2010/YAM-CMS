@using MyCMS.Web.Helpers

<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>

    <link href='http://fonts.googleapis.com/css?family=Noticia+Text:400,400italic,700' rel='stylesheet' type='text/css'>
    @*<link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />*@

    <link href="@Url.Stylesheet("styles.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Script("jquery-1.5.1.min.js")" type="text/javascript"></script>

</head>
<body>

    <div id="outer-header">
        <div id="header">
            <div id="login-display">
                @Html.Partial("_LogOnPartial")
            </div>
        </div>
    </div>

    <div id="wrapper">

        <div id="left-column">
            @RenderBody()
        </div>

        <div id="middle-column">@RenderSection("MiddleColumn", true)</div>

        <div id="right-column">@RenderSection("RightColumn", true)</div>

        <div id="footer">
        Copyright /*MyDevBlog*/ @DateTime.Now.Year, Todos los derechos reservados.
        </div>
    </div>
</body>
</html>
